{
  "name": "@mtr/ui",
  "private": true,
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist tsconfig.tsbuildinfo node_modules"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "import": "./src/client.ts"
    },
    "./css": {
      "default": "./src/css/globals.css"
    },
    "./tailwind-config": {
      "default": "./src/tailwind/config.ts"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@mtr/services": "workspace:*",
    "@mtr/utils": "workspace:*",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "ag-grid-community": "^34.1.0",
    "ag-grid-react": "^34.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.331.0",
    "next-themes": "^0.4.6",
    "react-hook-form": "^7.58.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5"
  }
}
